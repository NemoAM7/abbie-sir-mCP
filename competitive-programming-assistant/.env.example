# Environment Variables Configuration
# Copy this file to .env and fill in your actual values

# --- Security & Credentials ---
AUTH_TOKEN=your_auth_token_here
CLIST_API_KEY=your_clist_api_key_here
GEMINI_API_KEY=your_gemini_api_key_here

# --- User & Tool Configuration ---
MY_NUMBER=your_number_here
DEFAULT_HANDLE=your_codeforces_handle_here

# --- Render Keep-Alive Configuration (for deployment) ---
# Set this to your deployed Render URL + /health endpoint
# Example: https://your-app-name.onrender.com/health
# Leave empty for local development
RENDER_HEALTH_URL=
